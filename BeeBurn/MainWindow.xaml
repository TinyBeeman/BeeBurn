<Window x:Class="BeeBurn.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BeeBurn"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="200"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="0">
            <Button Click="ClickProject" Margin="10,10,10,0">Go</Button>
            <Button Click="ClickPaste" Margin="10,10,10,0">Paste</Button>
        </StackPanel>
        <StackPanel Grid.Column="2">
            <local:BeeImageList ImageList="{Binding ActiveImages}"/>
        </StackPanel>
        
    </Grid>
</Window>
